<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.codejumble.opentube.Main</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath com.codejumble.opentube.Main</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.codejumble.opentube.Main</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-javadoc</actionName>
            <displayName>javadoc</displayName>
            <goals>
                <goal>javadoc:javadoc</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-release</actionName>
            <displayName>release</displayName>
            <goals>
                <goal>release:prepare</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-release-perform</actionName>
            <displayName>release-perform</displayName>
            <goals>
                <goal>release:perform</goal>
            </goals>
        </action>
    </actions>
